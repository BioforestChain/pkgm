{
  "name": "@bfchain/pkgm-bfsp",
  "version": "2.5.5",
  "description": "",
  "type": "module",
  "types": "./src/index.ts",
  "main": "./dist/index.mjs",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com/"
  },
  "exports": {
    ".": {
      "import": "./dist/main/index.mjs"
    },
    "./bin": {
      "import": "./dist/main/bin.mjs"
    },
    "./test": {
      "import": "./dist/main/test.mjs"
    },
    "./postinstall": {
      "import": "./dist/script/postinstall.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "bfsp": "./dist/main/bfsp.bin.mjs"
  },
  "files": [
    "dist",
    "typings",
    "src",
    "bin",
    "assets",
    "*.ts",
    "*.mjs"
  ],
  "scripts": {
    "lsdeps": "yarn list --json --prod > .d.json",
    "postinstall": "node ./postinstall.mjs",
    "up": "yarn upgrade-interactive",
    "ndb": "ndb --experimental-modules ",
    "tsc": "tsc --build tsconfig.json -w",
    "dev:main": "vite build --watch --minify=false --sourcemap=true",
    "dev:script": "yarn dev:main -- --mode=script",
    "dev": "yarn dev:main",
    "build:main": "vite build --minify=false --sourcemap=true",
    "build:script": "yarn build:main -- --mode=script",
    "build": "yarn build:main & yarn build:script",
    "test": "ava"
  },
  "keywords": [],
  "author": "bnqkl",
  "license": "MIT",
  "dependencies": {
    "@bfchain/util-extends-map": "^0.0.1-alpha.4",
    "@bfchain/util-extends-promise-out": "^0.0.1-alpha.4",
    "ava": "^4.0.1",
    "blessed": "^0.1.81",
    "chalk": "^5.0.0",
    "debug": "^4.3.2",
    "fb-watchman": "^2.0.1",
    "ignore": "^5.2.0",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.20",
    "terser": "^5.9.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.19",
    "@types/debug": "^4.1.7",
    "@types/fb-watchman": "^2.0.1",
    "@types/node": "^16.0.0",
    "@types/prettier": "^2.4.2",
    "zx": "^5.2.0"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
